#!/bin/bash

if [[ $TERM == "dumb" ]] ; then
	unset PROMPT_COMMAND
	PS1='$ '
	return
fi
main(){
    p.ordenv
    if [[ "$-" == *i* ]] ; then
        source ~/.git-prompt-phil.sh
    fi
}

source $HOME/.philconfig/cmc_home/cmc_utils/p.setup_spooki.def.sh
source $HOME/.philconfig/cmc_home/cmc_utils/p.ordenv.def.sh

main
